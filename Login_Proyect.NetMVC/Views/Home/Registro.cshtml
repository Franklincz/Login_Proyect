@model Login_Proyect.NetMVC.Models.Usuario
<h1>Registro</h1>



<form asp-action="Registrar" asp-controller="Home" id="Registrar">
    <div class="row">


        <div class="form-group col-md-3">

            <label>Usuario</label>
            <input type="text" asp-for="Nombre" class="form-control" />
            <span asp-validation-for="Nombre"></span>



        </div>
    </div>

    <div class="row">


        <div class="form-group col-md-3" >

            <label>Password</label>
            <input type="password" asp-for="Clave" class="form-control" />
            <span asp-validation-for="Clave"></span>

        </div>
    </div>
    <div class="row">
        <button type="submit" class="btn btn-primary" id="btnRegistrar">REGISTRAR</button>
    </div>



</form>



